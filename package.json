{
  "name": "@pe/builder-editor",
  "version": "3.7.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:editor": "ng serve editor-app",
    "start:editor-lib": "ng build editor-lib --watch",
    "start:core": "ng build core-library --watch",
    "start:ui": "ng build ui-library --watch",
    "start:text-editor": "ng build text-editor-lib --watch",
    "start:text-editor-sandbox": "ng serve text-editor-sandbox",
    "build": "ng build",
    "build:editor": "ng build editor-app",
    "build:core": "ng build core-library",
    "build:ui": "ng build ui-library",
    "build:text-editor-compat": "ng build text-editor-lib_old",
    "build:text-editor": "ng build text-editor-lib",
    "build:all": "npm run build:core && npm run build:text-editor-compat && npm run build:text-editor && npm run build:ui",
    "publish:all": "cd dist/libs/core && npm publish && cd ../ui && npm publish && cd ../text-editor && npm publish && cd ../text-editor-compat && npm publish && cd ../../../ && npm publish",
    "link:core": "cd dist/libs/core && npm link && cd ../../../ && npm link @pe/builder-core",
    "link:text-editor": "cd dist/libs/text-editor && npm link && cd ../../../ && npm link @pe/builder-text-editor",
    "link:text-editor-compat": "cd dist/libs/text-editor-compat && npm link && cd ../../../ && npm link @pe/builder-text-editor-compat",
    "link:ui": "cd dist/libs/ui && npm link && cd ../../../ && npm link @pe/builder-ui",
    "test:core": "ng test core-library",
    "test": "ng test",
    "test:text-editor": "ng test text-editor-lib",
    "init": "npm run build:core && npm run link:core && npm run build:text-editor && npm run link:text-editor && npm run build:text-editor-compat && npm run link:text-editor-compat && npm run build:ui && npm run link:ui",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/common": "^9.0.4",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^9.0.4",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "^9.0.4",
    "@angular/router": "^9.0.4",
    "hammerjs": "^2.0.8",
    "hash.js": "^1.1.7",
    "html-janitor": "^2.0.4",
    "lodash-es": "^4.17.15",
    "ngx-color-picker": "^8.2.0",
    "ngx-highlight-js": "^9.0.0",
    "ngx-hm-carousel": "^2.0.0-rc.1",
    "ngx-indexed-db": "^4.0.14",
    "rxjs": "^6.5.3",
    "symbol-observable": "^1.2.0",
    "tslib": "^1.10.0",
    "typeface-open-sans": "0.0.75",
    "uuid": "^3.3.3",
    "zone.js": "~0.10.2"
  },
  "peerDependencies": {
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular-devkit/build-ng-packagr": "~0.900.4",
    "@angular/animations": "^9.0.4",
    "@angular/cdk": "^8.0.2",
    "@angular/cli": "^9.0.4",
    "@angular/compiler": "^9.0.4",
    "@angular/compiler-cli": "^9.0.4",
    "@angular/language-service": "^9.0.4",
    "@angular/platform-browser-dynamic": "^9.0.4",
    "@pe/feature-flag": "1.0.0",
    "@pe/frontend-dev-kit": "^0.1.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.11.1",
    "@types/uuid": "^3.4.6",
    "ang-jsoneditor": "^1.8.4",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsoneditor": "^8.1.0",
    "karma": "~4.4.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-json-viewer": "^2.4.0",
    "protractor": "~5.4.2",
    "puppeteer": "^2.0.0",
    "ts-node": "~8.5.4",
    "tslib": "^1.10.0",
    "tslint": "~5.20.1",
    "typescript": "~3.7.5",
    "yaml": "^1.7.2"
  }
}
